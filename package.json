{
  "name": "react-datatable-generator",
  "author": "sebastien delile",
  "keywords": [
    "react",
    "table",
    "component"
  ],
  "private": false,
  "version": "0.1.7",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SebDelile/react-datatable-generator"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@emotion/css": "^11.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-publish": "SET NODE_ENV=production && rm -rf dist && babel src/lib --out-dir dist --copy-files --ignore \"src/**/*.spec.js\",\"src/**/*.test.js\" --no-copy-ignored",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "jsdoc": "rm -rf docs && jsdoc src/lib -r -d docs --readme readme.md -c jsdoc.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "docdash": "^1.2.0",
    "jsdoc": "^3.6.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-script": "^2.0.5"
  }
}
